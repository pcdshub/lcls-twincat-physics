<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1">
  <POU Name="FB_FalliblePublic" Id="{b31e2ad8-b7ee-4d66-a0d2-490aab45eadc}" SpecialFunc="None">
    <Declaration><![CDATA[
FUNCTION_BLOCK FB_FalliblePublic IMPLEMENTS I_FalliblePublic
VAR
    {attribute 'pytmc' := '
        pv: Error
        io: i
        field: DESC TRUE if in error state. FALSE if not.
    '}
    bError : BOOL;
    {attribute 'pytmc' := '
        pv: ErrorMsg
        io: i
        field: DESC Message to aid in debugging if error is true.
    '}
    sErrorMsg : T_MaxString;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Method Name="ClearError" Id="{e508f329-7528-441e-8224-c35c1c1d76fc}">
      <Declaration><![CDATA[
METHOD ClearError
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
