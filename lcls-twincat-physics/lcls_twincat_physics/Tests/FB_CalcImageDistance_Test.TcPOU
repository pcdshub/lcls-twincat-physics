    <Method Name="TestValidInputsExpectNoErrorAccurateImageDistance" Id="{43ee5803-5362-4de9-bcf2-bf0ebf137973}">
      <Declaration><![CDATA[
METHOD TestValidInputsExpectNoErrorAccurateImageDistance
VAR_INPUT
    fFocalLength : LREAL;
    fSourceDistance : LREAL;

    fExpected : LREAL;
END_VAR
VAR
    fbFocalLength : FB_QuantityLREALPublicMock;
    fbSourceDistance: FB_QuantityLREALPublicMock;
    fbExpected : FB_QuantityLREALPublicMock;
    fbConvertUnitsLREALMock : FB_ConvertUnitsLREALMock;

    // Subject under test
    fbCalcImageDistance : FB_CalcImageDistance(ConvertUnits := fbConvertUnitsLREALMock);
    fbResult : FB_QuantityLREALPublic(Units := 'm');
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
TEST(CONCAT(STR1 := 'TestValidInputsExpectNoErrorAccurateImageDistance ID: ',
            STR2 := INT_TO_STRING(nTestID)));

// Test Fixture
SetInitialFocalLengthAndSourceDistanceValues(
    FocalLength := fbFocalLength,
    SourceDistance := fbSourceDistance
);

fbFocalLength.Val := fFocalLength;
fbSourceDistance.Val := fSourceDistance;

fbExpected.Val := fExpected;
fbExpected.Units := fbResult.Units;

// Subject Under Test
fbCalcImageDistance.Calculate(
    FocalLength := fbFocalLength,
    SourceDistance := fbSourceDistance,
    ImageDistance := fbResult
);

AssertNoError(Fallible := fbCalcImageDistance);

AssertNoErrorExpectedQuantityLREAL(
    Expected := fbExpected,
    Actual := fbResult,
    Delta := ABS(0.001 * fbExpected.Val)
);

TEST_FINISHED();

nTestID := nTestID + 1;
]]></ST>
      </Implementation>
