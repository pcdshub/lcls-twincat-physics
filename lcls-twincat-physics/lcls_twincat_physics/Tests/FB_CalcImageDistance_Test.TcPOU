    <Method Name="SetInitialFocalLengthAndSourceDistanceValues" Id="{6d389205-7189-4927-8632-4bf5c22a96fa}">
      <Declaration><![CDATA[
METHOD PRIVATE SetInitialFocalLengthAndSourceDistanceValues
VAR_IN_OUT
    FocalLength : FB_QuantityLREALPublicMock;
    SourceDistance : FB_QuantityLREALPublicMock;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
FocalLength.Val := 1.0;
FocalLength.Units := 'm';
FocalLength.ClearError();

SourceDistance.Val := 1.0;
SourceDistance.Units := 'm';
SourceDistance.ClearError();
]]></ST>
      </Implementation>
    </Method>
    <Method Name="Test0FocalLengthExpectError0ImageDistance" Id="{4e7b4f83-062f-4494-90d5-114858ee4334}">
      <Declaration><![CDATA[
METHOD PRIVATE Test0FocalLengthExpectError0ImageDistance
VAR
    fbFocalLength : FB_QuantityLREALPublicMock;
    fbSourceDistance: FB_QuantityLREALPublicMock;
    fbExpected : FB_QuantityLREALPublicMock;
    fbConvertUnitsLREALMock : FB_ConvertUnitsLREALMock;

    // Subject under test
    fbCalcImageDistance : FB_CalcImageDistance(ConvertUnits := fbConvertUnitsLREALMock);
    fbResult : FB_QuantityLREALPublic(Units := 'm');
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
TEST('Test0FocalLengthExpectError0ImageDistance');

// Test fixture
SetInitialFocalLengthAndSourceDistanceValues(
    FocalLength := fbFocalLength,
    SourceDistance := fbSourceDistance
);

fbFocalLength.Val := 0.0;

fbExpected.Val := fbResult.Val;
fbExpected.Units := fbResult.Units;

// Here the test starts
fbCalcImageDistance.Calculate(
    FocalLength := fbFocalLength,
    SourceDistance := fbSourceDistance,
    ImageDistance := fbResult
);

AssertError(Fallible := fbCalcImageDistance);

AssertErrorExactExpectedQuantityLREAL(
    Expected := fbExpected,
    Actual := fbResult
);

TEST_FINISHED();
]]></ST>
      </Implementation>
    </Method>
