    <Method Name="TestSetInvalidNumLensesExpectErrorNoChangeToValue" Id="{6e1a08f1-05bf-4a64-a3f2-0ce2e1fad65f}">
      <Declaration><![CDATA[
METHOD TestSetInvalidNumLensesExpectErrorNoChangeToValue
VAR
    // Subject under test
    fbLensSet : FB_LensSet;
